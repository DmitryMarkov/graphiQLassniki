{
  "name": "graphiqlassniki",
  "version": "0.1.1",
  "description": "GraphQL + React + Apollo playground",
  "main": "index.js",
  "scripts": {
    "client": "webpack-dev-server --devtool inline-sourse-map --hot --config webpack.client.config.js",
    "client:build": "webpack --config webpack.client.build.config.js",
    "client:analyze": "webpack-bundle-analyzer stats.json",
    "client:stats": "webpack --profile --json --config webpack.client.build.config.js > stats.json",
    "test": "yarn client:build && yarn server",
    "sequelize:migrate": "dotenv -- sequelize db:migrate --migrations-path ./src/server/migrations --config ./src/server/config/index.js",
    "sequelize:undo": "dotenv -- sequelize db:migrate:undo --migrations-path ./src/server/migrations --config ./src/server/config/index.js",
    "sequelize:reset": "dotenv -- sequelize db:migrate:undo:all --migrations-path ./src/server/migrations --config ./src/server/config/index.js",
    "sequelize:seed": "dotenv -- sequelize db:seed:all --seeders-path ./src/server/seeders --config ./src/server/config/index.js",
    "server": "nodemon --exec dotenv babel-node --watch src/server src/server/index.js",
    "upgrade-dependencies": "yarn upgrade-interactive --latest --exact",
    "watch": "run-p client server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DmitryMarkov/graphiQLassniki.git"
  },
  "keywords": [],
  "author": "Dmitry Markov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DmitryMarkov/graphiQLassniki/issues"
  },
  "homepage": "https://github.com/DmitryMarkov/graphiQLassniki#readme",
  "resolutions": {
    "lodash": "4.17.15"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link": "1.2.13",
    "apollo-link-error": "1.1.12",
    "apollo-link-http": "1.5.16",
    "apollo-server-express": "2.14.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.6",
    "helmet": "3.21.2",
    "lodash": "4.17.15",
    "mysql2": "1.6.5",
    "nodemon": "2.0.2",
    "react": "16.12.0",
    "react-apollo": "3.1.3",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "sequelize": "5.21.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/node": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-decorators": "7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "7.7.4",
    "@babel/plugin-proposal-function-sent": "7.7.4",
    "@babel/plugin-proposal-numeric-separator": "7.7.4",
    "@babel/plugin-proposal-throw-expressions": "7.7.4",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.4.0",
    "dotenv-cli": "3.1.0",
    "dotenv-webpack": "1.7.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "3.0.3",
    "eslint-plugin-react": "7.17.0",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "style-loader": "1.1.2",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  }
}
