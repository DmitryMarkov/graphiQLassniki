{
  "name": "graphiqlassniki",
  "version": "0.1.1",
  "description": "GraphQL + React + Apollo playground",
  "main": "index.js",
  "scripts": {
    "client": "webpack-dev-server --devtool inline-sourse-map --hot --config webpack.client.config.js",
    "client:build": "webpack --config webpack.client.build.config.js",
    "client:analyze": "webpack-bundle-analyzer stats.json",
    "client:stats": "webpack --profile --json --config webpack.client.build.config.js > stats.json",
    "test": "yarn client:build && yarn server",
    "sequelize:migrate": "dotenv -- sequelize db:migrate --migrations-path ./src/server/migrations --config ./src/server/config/index.js",
    "sequelize:undo": "dotenv -- sequelize db:migrate:undo --migrations-path ./src/server/migrations --config ./src/server/config/index.js",
    "sequelize:reset": "dotenv -- sequelize db:migrate:undo:all --migrations-path ./src/server/migrations --config ./src/server/config/index.js",
    "sequelize:seed": "dotenv -- sequelize db:seed:all --seeders-path ./src/server/seeders --config ./src/server/config/index.js",
    "server": "nodemon --exec dotenv babel-node --watch src/server src/server/index.js",
    "upgrade-dependencies": "yarn upgrade-interactive --latest --exact",
    "watch": "run-p client server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DmitryMarkov/graphiQLassniki.git"
  },
  "keywords": [],
  "author": "Dmitry Markov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DmitryMarkov/graphiQLassniki/issues"
  },
  "homepage": "https://github.com/DmitryMarkov/graphiQLassniki#readme",
  "resolutions": {
    "http-proxy": ">=1.18.1",
    "lodash": ">=4.17.20",
    "kind-of": ">=6.0.3",
    "minimist": ">=1.2.3",
    "node-forge": ">=0.10.0",
    "serialize-javascript": ">=3.1.0",
    "yargs": ">=13.3.2"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "apollo-server-express": "2.18.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "graphql-tag": "2.11.0",
    "graphql-tools": "4.0.6",
    "helmet": "3.23.0",
    "lodash": "4.17.20",
    "mysql2": "1.6.5",
    "nodemon": "2.0.4",
    "react": "16.13.1",
    "react-apollo": "3.1.5",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "sequelize": "5.21.13",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-proposal-export-namespace-from": "7.10.4",
    "@babel/plugin-proposal-function-sent": "7.10.4",
    "@babel/plugin-proposal-numeric-separator": "7.10.4",
    "@babel/plugin-proposal-throw-expressions": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.6.0",
    "dotenv-cli": "3.1.0",
    "dotenv-webpack": "1.8.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.21.3",
    "file-loader": "6.1.0",
    "html-webpack-plugin": "4.5.0",
    "mini-css-extract-plugin": "0.12.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
